{
  "name": "npm-pkgbuild",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/npm-pkgbuild.js",
  "module": "src/npm-pkgbuild.mjs",
  "description": "create arch linux package from npm",
  "keywords": [
    "aur",
    "pkgbuild",
    "arch-linux"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "npm-pkgbuild": "bin/npm-pkgbuild"
  },
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme src/npm-pkgbuild.mjs --section=API",
    "lint": "documentation lint src/npm-pkgbuild.mjs",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "caporal": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^1.0.0-rc.2",
    "documentation": "^8.1.2",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.67.3",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "semantic-release": "^15.12.1",
    "stream-buffers": "^3.0.2",
    "travis-deploy-once": "^5.0.9",
    "c8": "^3.2.1",
    "esm": "^3.0.84"
  },
  "engines": {
    "node": ">=10.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-pkgbuild.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-pkgbuild/issues"
  },
  "homepage": "https://github.com/arlac77/npm-pkgbuild#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "extensions": [
      "js",
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
