{
  "name": "npm-pkgbuild",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/pkgbuild.mjs",
  "module": "src/pkgbuild.mjs",
  "description": "create ArchLinux packages from npm packages",
  "keywords": [
    "aur",
    "pkgbuild",
    "arch-linux",
    "ArchLinux"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "npm-pkgbuild": "bin/npm-pkgbuild"
  },
  "scripts": {
    "cover": "c8 -x 'tests/**/*.mjs' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/pkgbuild.mjs --section=API",
    "lint": "documentation lint src/pkgbuild.mjs",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "test": "ava"
  },
  "dependencies": {
    "commander": "^3.0.0",
    "execa": "^2.1.0",
    "expression-expander": "^7.0.2",
    "globby": "^10.0.1",
    "iterable-string-interceptor": "^1.0.1"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "builtin-modules": "^3.1.0",
    "c8": "^5.0.4",
    "documentation": "^12.1.2",
    "esm": "^3.2.25",
    "markdown-doctest": "^1.0.0",
    "rollup": "^1.23.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-executable": "^1.5.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "semantic-release": "^15.13.26",
    "stream-buffers": "^3.0.2"
  },
  "engines": {
    "node": ">=12.11.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-pkgbuild.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-pkgbuild/issues"
  },
  "homepage": "https://github.com/arlac77/npm-pkgbuild#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "extensions": [
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
